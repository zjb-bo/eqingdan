<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>

    </style>

    <style name="fiveStarRatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:maxHeight">30.0dip</item>
        <item name="android:progressDrawable">@drawable/five_rating_bar</item>
        <item name="android:minHeight">15.0dip</item>
        <item name="android:numStars">5</item>
        <item name="android:rating">0.0</item>
        <item name="android:stepSize">0.1</item>
    </style>

</resources>
